{
  "kind": "collectionType",
  "collectionName": "cards",
  "info": {
    "singularName": "card",
    "pluralName": "cards",
    "displayName": "Card",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "character": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::character.character"
    },
    "influence": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true
    },
    "defense": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "mappedBy": "relate_cards"
    },
    "img_ref": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "unique": false,
      "required": true
    },
    "attribute": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "LOGIC",
        "EMPATHY",
        "INTUITION"
      ],
      "required": true
    },
    "card_acquisitions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::card-acquisition.card-acquisition",
      "inversedBy": "cards"
    },
    "rarity": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::rarity.rarity",
      "inversedBy": "cards"
    },
    "thumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
